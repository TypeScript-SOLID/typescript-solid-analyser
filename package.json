{
  "name": "ts-solid-analyser",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "frontend:build": "npm run build --prefix apps/frontend",
    "frontend:dev": "npm start --prefix apps/frontend",
    "frontend:install": "npm install --prefix apps/frontend",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/ts-solid-analyzer/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-ws": "^7.4.4",
    "@nestjs/websockets": "^7.4.4",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie-session": "^1.4.0",
    "csurf": "^1.11.0",
    "extract-zip": "^2.0.1",
    "mongoose": "^5.10.3",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "tmp-promise": "^3.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.0.0",
    "@types/cookie-session": "^2.0.41",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.3",
    "@types/jest": "^26.0.14",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.10.3",
    "@types/rimraf": "^3.0.0",
    "@types/supertest": "^2.0.8",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "http-proxy-middleware": "^1.0.5",
    "jest": "26.4.2",
    "lerna": "^3.22.1",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@tssa/common-auth/(.*)": "<rootDir>/libs/common/src/$1",
      "@tssa/common-auth": "<rootDir>/libs/common/src"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
